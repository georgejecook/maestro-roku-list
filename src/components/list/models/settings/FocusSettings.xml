<?xml version='1.0' encoding='utf-8'?>
<component
    name='ml_FocusSettings'
    extends='Node'>
    <interface>
      <field id='animSpeed' type='float'/>
      <field id='fastAnimSpeed' type='float'/>
      <!-- fixed|floating -->
      <field id='focusStyle' type='string' value='fixed'/>

      <!-- onTop|underneath|none -->
      <field id='focusFeedbackStyle' type='string' value='onTop'/>
      <field id='focusFeedbackUri' type='string' value='pkg:/images/input-focusrect.9.png'/>
      <field id='focusFeedbackColor' type='string' value='#ffffff'/>
    </interface>
</component>