<?xml version='1.0' encoding='utf-8'?>

<component
    name='CellProvider'
    extends='Group'>

  <interface>
    <field
        id='_debugM'
        type='boolean'
        alwaysNotify='true' />
    <field
        id='__m'
        type='assocarray' />
    <field
        id='isCreateMoreEnabled'
        type='boolean'
        value='true' />
    <field
        id='numerOfCellsToCreateWhenDry'
        type='integer'
        value='30' />
    <field
        id='numberOfCellsInUse'
        type='integer'
        value='0' />
    <field
        id='numberOfCells'
        type='integer'
        value='0' />

    <function name='addPreBakedCells' />
    <function name='reset' />
    <function name='getCell' />
    <function name='releaseCell' />
  </interface>
</component>