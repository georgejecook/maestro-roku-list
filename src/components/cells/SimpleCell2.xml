<?xml version='1.0' encoding='utf-8'?>

<component
    name='SimpleCell2'
    extends='Group'>

  <!-- Note that target and rect are both set during the interpolation -->
  <interface>
    <field
        id='index'
        type='int' />
    <field
        id='itemContent'
        type='node' />
    <field
        id='focusPercent'
        type='float' />

  </interface>


  <children>
    <Rectangle
        id='theRect'
        width='300'
        height='300'
        color='#8034eb'>
      <Label
          id='theLabel'
          color='#000000'
          translation='[10, 10]' />

      <Label
          id='theLabel2'
          text='OTHER'
          color='#000000'
          translation='[10, 30]' />
    </Rectangle>
    <ColorFieldInterpolator
        id='focusColorInterp'
        key='[0.0, 1.0]'
        keyValue='[0x00002080, 0x000080FF]' />
  </children>

</component>